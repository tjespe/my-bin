sleep 0.5
spd-say "wiiiiiiiee"
sleep 0.5
spd-say "wiiiiiiiee"
sleep 0.5
spd-say "wiiiiiiiee"
sleep 0.2
spd-say "wiiiiiiiee"
sleep 0.2
spd-say "wiiiiiiiee"
sleep 0.2
spd-say "wiiiiiiiee"
sleep 0.2
spd-say "wiiiiiiiee"
sleep 0.1
spd-say "wiiiiiiiee"
